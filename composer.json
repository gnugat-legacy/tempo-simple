{
    "name": "gnugat/tempo-simple",
    "license": "MIT",
    "type": "project",
    "description": "A time tracking software",
    "autoload": {
        "psr-4": {
            "": "src/",
            "TempoSimple\\Test\\Functional\\Cli\\": "test/functional/cli",
            "TempoSimple\\Test\\Functional\\Web\\": "test/functional/web"
        }
    },
    "require": {
        "php": ">=5.3.3",

        "symfony/symfony": "~2.4",
        "symfony/console": "~2.5@dev",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~2.3",
        "incenteev/composer-parameter-handler": "~2.0",
        "sensio/framework-extra-bundle": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "phpspec/phpspec": "~2.0@rc"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
